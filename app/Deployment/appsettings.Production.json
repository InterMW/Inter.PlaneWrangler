{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Timing": {
        "CompilationOffsetSecs": 1,
        "CompilationDurationPredictionSecs": 1,
        "PlaneDocLifetimesSecs": 45
    },
    "AllowedHosts": "plane.centurionx.net",
    "Application": {
        "Name": "Inter.PlaneWrangler",
        "Version": "0.0.0.0"
    },
    "PlaneCacheContext": {
        "Uri": "#{ONPREM_REDIS}"
    },
    "InfluxDBContext": {
        "Uri": "#{INFLUXDB_TOKEN}"
    },
    "Rabbit": {
        "Scale": 1,
        "ClientDeclarations": {
            "Connections": [
                {
                    "Name": "Connection",
                    "ClientName": "Inter.PlaneWrangler",
                    "ServerName": "10.0.0.215",
                    "UserName": "dev_secure",
                    "Password": "#{RABBIT_PASSWORD}"
                }
            ],
            "AsyncRecievers": [
                {
                    "Name": "TickMessage",
                    "Connection": "Connection",
                    "Queue": "inter_plane_wranger_tick"
                },
                {
                    "Name": "PlaneFrameMessage",
                    "Connection": "Connection",
                    "Queue": "inter_plane_wranger_ingress"
                }
            ],
            "Publishers": [
                {
                    "Name": "CompletedPlaneFrameMessage",
                    "Connection": "Connection",
                    "Exchange": "InterTopic"
                },
                {
                    "Name": "MetricMessage",
                    "Connection": "Connection",
                    "Exchange": "InterTopic"
                }
            ]
        },
        "ServerDeclarations": {
            "Exchanges": [
                {
                    "Name": "Clock",
                    "Type": "Topic",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Connection": "Connection"
                },
                {
                    "Name": "InterTopic",
                    "Type": "Topic",
                    "AutoDelete": "False",
                    "Durable": "True",
                    "Connection": "Connection"
                }
            ],
            "Bindings": [
                {
                    "Queue": "inter_plane_wranger_tick",
                    "Connection": "Connection",
                    "Exchange": "Clock",
                    "SubscriptionKey": "tick.second"
                },
                {
                    "Queue": "inter_plane_wranger_ingress",
                    "Connection": "Connection",
                    "Exchange": "InterTopic",
                    "SubscriptionKey": "plane.proto"
                }
            ],
            "Queues": [
                {
                    "Name": "inter_plane_wranger_tick",
                    "Connection": "Connection",
                    "AutoDelete": "True",
                    "Durable": "False",
                    "Exclusive": "False"
                },
                {
                    "Name": "inter_plane_wranger_ingress",
                    "Connection": "Connection",
                    "AutoDelete": "True",
                    "Durable": "False",
                    "Exclusive": "False"
                }
            ]
        }
    }
}
