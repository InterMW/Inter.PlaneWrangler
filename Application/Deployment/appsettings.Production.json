{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Timing":
  {
    "CompilationOffsetSecs": 1,
    "CompilationDurationPredictionSecs": 1,
    "PlaneDocLifetimesSecs": 45
  },
  "AllowedHosts": "*",
  "Application":
  {
      "Name": "Inter.PlaneWrangler",
      "Version": "0.0.0.0"
  },
  "ConnectionStrings" : {
    "PlaneCacheContext" : "#{ONPREM_REDIS}",
    "InfluxDBContext" : "#{INFLUXDB_TOKEN}"
  },
  "Rabbit":
  {
      "Scale": 1,
      "ClientDeclarations": 
      {
          "Connections":
          [
              {
                  "Name":"IngressConnection",
                  "ClientName": "Inter.PlaneWrangler",
                  "ServerName": "10.0.0.215",
                  "UserName": "dev_secure",
                  "Password": "#{RABBIT_PASSWORD}"
              },
              {
                  "Name":"TickConnection",
                  "ClientName": "Inter.PlaneWrangler",
                  "ServerName": "10.0.0.215",
                  "UserName": "dev_secure",
                  "Password": "#{RABBIT_PASSWORD}"
              },
              {
                  "Name":"OutboundConnection",
                  "ClientName": "Inter.PlaneWrangler",
                  "ServerName": "10.0.0.215",
                  "UserName": "dev_secure",
                  "Password": "#{RABBIT_PASSWORD}"
              }
          ],
          "AsyncRecievers":
          [
              {
                  "Name": "TickMessage",
                  "Connection": "TickConnection",
                  "Queue": "inter_plane_wranger_tick"
              },
              {
                  "Name": "PlaneFrameMessage",
                  "Connection": "IngressConnection",
                  "Queue": "inter_plane_wranger_ingress"
              }
          ],
          "Publishers":
          [
            {
              "Name": "CompletedPlaneFrameMessage",
              "Connection": "OutboundConnection",
              "Exchange": "InterTopic"
            },
            {
                "Name": "MetricMessage",
                "Connection": "OutboundConnection",
                "Exchange": "InterTopic"
            }
          ]
      },
      "ServerDeclarations":
      {
          "Exchanges":
          [
              {
                  "Name": "Clock",
                  "Type": "Topic",
                  "AutoDelete": "False",
                  "Durable": "True",
                  "Connection": "TickConnection"
              },
              {
                  "Name": "InterTopic",
                  "Type": "Topic",
                  "AutoDelete": "False",
                  "Durable": "True",
                  "Connection": "IngressConnection"
              }
          ],
          "Bindings":
          [
              {
                  "Queue": "inter_plane_wranger_tick",
                  "Connection": "TickConnection",
                  "Exchange": "Clock",
                  "SubscriptionKey": "tick.second"
              },
              {
                  "Queue": "inter_plane_wranger_ingress",
                  "Connection": "IngressConnection",
                  "Exchange": "InterTopic",
                  "SubscriptionKey": "plane.proto"
              }
          ],
          "Queues":
          [
              {
                  "Name": "inter_plane_wranger_tick",
                  "Connection": "TickConnection",
                  "AutoDelete": "True",
                  "Durable": "False",
                  "Exclusive": "False"
              },
              {
                  "Name": "inter_plane_wranger_ingress",
                  "Connection": "IngressConnection",
                  "AutoDelete": "True",
                  "Durable": "False",
                  "Exclusive": "False"
              }
          ]
      } 
  }
}